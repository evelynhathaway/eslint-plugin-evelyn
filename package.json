{
  "name": "eslint-plugin-evelyn",
  "version": "10.1.0",
  "description": "ESLint plugin for my projects with my preferred code style and bugfixes",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-plugin"
  ],
  "homepage": "https://github.com/evelynhathaway/eslint-plugin-evelyn#readme",
  "bugs": {
    "url": "https://github.com/evelynhathaway/eslint-plugin-evelyn/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evelynhathaway/eslint-plugin-evelyn.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./files": "./lib/files.js"
  },
  "main": "./lib/index.js",
  "directories": {
    "lib": "/lib"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "lint": "eslint",
    "prepare": "husky"
  },
  "commitlint": {
    "extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "./": "eslint --fix"
  },
  "release": {
    "extends": "conventional-changelog-evelyn/release.config.js"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^3.1.0",
    "commitlint": "^19.7.1",
    "conventional-changelog-evelyn": "^2.0.1",
    "eslint": "^9.20.1",
    "eslint-import-resolver-typescript": "^3.8.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "semantic-release": "^24.2.3",
    "sort-package-json": "^2.14.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^15.0.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "eslint": "^9.20.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-regexp": "^2.0.0",
    "eslint-plugin-testing-library": "^7.0.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
